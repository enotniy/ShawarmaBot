{
  "name": "enotniy/ShawarmaBot",
  "description": "PHP Wrapper for Telegram Bot API",
  "keywords": [
    "php",
    "telegram",
    "bot",
    "bot api"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "telegram-bot/api": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "TelegramBot\\Api\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}